{
  "name": "${projectName}",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-validate": "^2.0.0",
    "koa-logger": "^3.2.0",
    "koa-compress": "2.0.0",
    "koa-requestid": "2.0.1",
    "lodash": "^4.17.4",
    "egg-validate-plus": "^1.1.6"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start <%= '--env=${ENV:-prod} --port=${PORT:-7001} --workers=${WORKERS:-4}' %> --title=${projectName}",
    "stop": "egg-scripts stop --title=${projectName}",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "postinstall": "npm link ./modules/domain"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "${author}",
  "license": "MIT"
}
